<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DateSelection">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cal_button_icon"
        android:label="Date Selection"
        android:roundIcon="@drawable/cal_button_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewDesign">
        <activity android:name="com.example.DateSelection.CalendarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>